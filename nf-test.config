config {
    testsDir "tests"
    workDir ".nf-test"
    configFile "tests/nextflow.config"
    libDir "tests/data"
    profile "docker"
    withTrace true
    autoSort false
    ignore "modules/**", "subworkflows/**"
    options "-dump-channels"
    plugins {
        load "nft-utils@0.0.3"
    }
}
